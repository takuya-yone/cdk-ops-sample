{
  "name": "cdk-ops-sample",
  "version": "0.1.1",
  "bin": {
    "cdk-ops-sample": "bin/cdk-ops-sample.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "check": "biome check --write .",
    "pytest": "uv run pytest -vv -s tests",
    "py:fix": "./python_fix.sh",
    "ts:fix": "biome check lib src tests --write",
    "jest:cov": "jest --coverage",
    "ecr:login": "aws ecr-public get-login-password --region us-east-1 | docker login --username AWS --password-stdin public.ecr.aws"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.6",
    "@types/jest": "^30.0.0",
    "@types/node": "24.0.10",
    "aws-cdk": "2.1020.2",
    "esbuild": "^0.25.5",
    "jest": "^30.0.4",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3",
    "zod": "^3.25.74"
  },
  "dependencies": {
    "@aws-cdk/aws-lambda-python-alpha": "2.204.0-alpha.0",
    "@aws-lambda-powertools/logger": "^2.23.0",
    "@aws-sdk/client-sqs": "^3.840.0",
    "aws-cdk-lib": "2.204.0",
    "aws-sdk-client-mock": "^4.1.0",
    "aws-sdk-client-mock-jest": "^4.1.0",
    "constructs": "^10.4.2"
  },
  "packageManager": "pnpm@10.12.4"
}