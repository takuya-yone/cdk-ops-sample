{
  "name": "cdk-ops-sample",
  "version": "0.1.1",
  "bin": {
    "cdk-ops-sample": "bin/cdk-ops-sample.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "check": "biome check bin lib src tests",
    "py:fix": "./python_fix.sh",
    "py:test": "uv run pytest -vv -s tests",
    "ts:fix": "biome check bin lib src tests --write",
    "ts:test": "jest --coverage",
    "all:test": "pnpm py:test && pnpm ts:test",
    "all:fix": "pnpm py:fix && pnpm ts:fix",
    "ecr:login": "aws ecr-public get-login-password --region us-east-1 | docker login --username AWS --password-stdin public.ecr.aws"
  },
  "devDependencies": {
    "@biomejs/biome": "2.4.4",
    "@types/jest": "^30.0.0",
    "@types/node": "25.3.1",
    "aws-cdk": "2.1107.0",
    "esbuild": "^0.27.3",
    "jest": "^30.2.0",
    "jsonc-parser": "^3.3.1",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3",
    "zod": "^4.3.6"
  },
  "dependencies": {
    "@aws-cdk/aws-lambda-python-alpha": "2.240.0-alpha.0",
    "@aws-lambda-powertools/logger": "^2.31.0",
    "@aws-sdk/client-sqs": "^3.998.0",
    "aws-cdk-lib": "2.240.0",
    "aws-sdk-client-mock": "^4.1.0",
    "aws-sdk-client-mock-jest": "^4.1.0",
    "constructs": "^10.5.1"
  },
  "packageManager": "pnpm@10.30.2"
}